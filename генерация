
function generatePhoto(id) {
    return {
        id: id,
        url: `photos/${id}.jpg`,
        description: "Моя новая фотография",
        likes: getLikes(),
        comments: getComments()
    }
}
function generationComments() {
    return {
        id: "",
        avatar: `img/avatar${-getAvatar()}.svg`,
        message: "Всё отлично!",
        name: names
    }
}
